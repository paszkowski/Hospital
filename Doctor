package a;

public class Doctor extends Person {
	
	private int bonus;

	public int getBonus() {
		return bonus;
	}
	
	Doctor () {}
	
	Doctor(String imie, String nazwisko, int pay, int bonus) {
		super(imie, nazwisko, pay);
		this.bonus = bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}
	
	@Override
	public String toString() {
		
		return super.toString() + "Bonusowe hajsy: " + bonus;
	}

}
