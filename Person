package a;

public class Person {
	
	private String imie;
	private String nazwisko;
	private int pay;
	
	Person() {}
	
	Person(String imie, String nazwisko, int pay) {
		
		this.imie = imie;
		this.nazwisko = nazwisko;
		this.pay = pay;
		
	}
	
	public String getImie() {
		return imie;
	}
	public void setImie(String imie) {
		this.imie = imie;
	}
	public String getNazwisko() {
		return nazwisko;
	}
	public void setNazwisko(String nazwisko) {
		this.nazwisko = nazwisko;
	}
	public int getPay() {
		return pay;
	}
	public void setPay(int pay) {
		this.pay = pay;
	}
	
	@Override
	public String toString() {
		
		return "Imie: " + imie + " " + "Nazwisko: " + nazwisko + " " + "Placa: " + pay + " ";
	}
	
	

}
